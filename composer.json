{
    "name": "pqmtool/pqmtool",
    "description": "Tiny tool for parsing questions to moodle xml format",
    "keywords": ["moodle"],
    "authors": [
        {
            "name": "Nikita Maksimov",
            "email": "provoker.1986@gmail.com"
        }
    ],
    "bin": [
        "bin/pqmtool"
    ],
    "require": {
        "php": ">=7.4.3",
        "twig/twig": "^3.0",
        "php-di/php-di": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "PQMTool\\": "src"
        },
        "files": [
            "src/Lib/parse.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload-dev": {
        "psr-4": {
            "PQMTool\\": "Tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
