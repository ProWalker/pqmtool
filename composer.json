{
    "name": "pqmtool/pqmtool",
    "description": "Tiny tool for parsing questions to moodle xml format",
    "keywords": ["moodle"],
    "authors": [
        {
            "name": "Nikita Maksimov",
            "email": "provoker.1986@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.3"
    },
    "autoload": {
        "psr-4": {
